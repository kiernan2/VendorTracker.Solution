@using VendorTracker.Models;
@using System;

<h3>Here are all the Orders for this Vendor:</h3>

<ol>
  @foreach (Order order in @Model.Orders)
  {
    <li><a href="/vendor/@Model.Id/orders/@order.Id">@order.Details</a></li>
  }
</ol>

<p><a href='/vendor/@Model.Id/orders/new'>Add a new Order</a></p>
<p><a href="/vendor">Return to Vendor list</a></p>
<p><a href="/">Return to Main Page</a></p>