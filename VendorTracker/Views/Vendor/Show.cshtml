@using VendorTracker.Models;

<h3>Here are all the Orders for this Vendor:</h3>

<ol>
  @foreach (Order order in @Model.Order)
  {
    <li><a href="/vendor/@Model.Id/order/@order.Id"></a></li>
  }
</ol>

<p><a href="/vendor/@Model.Id/order/new">Add a new Order</a></p>
<p><a href="/vendor">Return to Vendor list</a></p>
<p><a href="/">Return to Main Page</a></p>